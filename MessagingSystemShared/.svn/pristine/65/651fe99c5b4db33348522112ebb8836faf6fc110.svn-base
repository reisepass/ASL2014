package edu.ethz.user04.shared.requests.messagerequests;

public class ReadMessageRequest {
	int senderID;
	int queueID;
	boolean retrieve;
	int anyOrEarliestOrImportant;
	public ReadMessageRequest(int sId, int qId, boolean retrieve, int anyOrEarliestOrImportant){
		senderID = sId;
		queueID = qId;
		this.retrieve = retrieve;
		this.anyOrEarliestOrImportant = anyOrEarliestOrImportant;
	}

}
